{
  "name": "thedivemap",
  "version": "0.0.1",
  "description": "The Dive Map is a map of the world that shows the locations of dive sites.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "api": "playwright test --project=api-chromium",
    "e2e": "playwright test --project=e2e-chromium --project=e2e-mobile-chrome",
    "e2e:desktop": "playwright test --project=e2e-chromium",
    "e2e:mobile": "playwright test --project=e2e-mobile-chrome",
    "test:playwright": "playwright test",
    "test:api": "playwright test --project=api-chromium",
    "test:e2e": "playwright test --project=e2e-chromium --project=e2e-mobile-chrome",
    "e2e:ui": "playwright test --project=e2e-chromium --project=e2e-mobile-chrome --ui",
    "e2e:headed": "playwright test --project=e2e-chromium --project=e2e-mobile-chrome --headed",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "report:clean": "rm -rf allure-report allure-results test-results"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.0",
    "clsx": "^2.1.1",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jsdom": "^26.1.0",
    "maplibre-gl": "^5.6.2",
    "next": "15.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.6.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "4.7.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.3.2",
